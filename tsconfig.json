{
  "compilerOptions": {
    "target": "es2016",                                 
    "removeComments": true,
    "module": "commonjs",                               
    "rootDir": "./",                                  
    "baseUrl": "./",                               
    // "paths": {},                                   
    // "rootDirs": [],                                
    "typeRoots": [
      "./customType",
      "node_modules/@types"
    ],                                  
      "outDir": "./dist",                          
    "esModuleInterop": true,                           
    "forceConsistentCasingInFileNames": true,          
    "strict": true,                                    
    "skipLibCheck": true                               
  }
}
